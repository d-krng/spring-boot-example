{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "dkrng-api",
      "image": "dkrng/dkrng-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "dbc:postgresql://TODO:5432/customer"
        }
      ]
    }
  ]
}